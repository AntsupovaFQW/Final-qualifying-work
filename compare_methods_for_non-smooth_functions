from math import exp, sin
from scipy.optimize import differential_evolution, minimize


def f(x):
    return int(sin(x / 5) * exp(x / 10) + 5 * exp(-x / 2))


def search_min_BFGS(x0):
    res = minimize(f, x0, method='BFGS')
    return res['fun']


def search_min_evolution(bounds):
    res = differential_evolution(f, bounds)
    return res['fun']

print(search_min_BFGS(30), search_min_evolution([(1, 30)]))
